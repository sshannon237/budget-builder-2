{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "budget-builder": {
      "appLocation": "client",
      "apiLocation": "api",
      "outputLocation": "client/build",
      "appBuildCommand": "npm run build",
      "apiBuildCommand": "npm run build --if-present",
      "run": "npm start",
      "appDevserverUrl": "http://localhost:3000"
    }
  }
}
